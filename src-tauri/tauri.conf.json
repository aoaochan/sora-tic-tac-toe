{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "sora-tic-tac-toe",
  "version": "0.1.0",
  "identifier": "com.sora-tic-tac-toe.game",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Sora TicTacToe",
        "width": 1024,
        "height": 768,
        "resizable": false,
        "maximizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico",
      "icons/icon.png"
    ]
  }
}
